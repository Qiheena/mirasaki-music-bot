{
  "name": "rasavedic-music-bot",
  "description": "A free, open-source JavaScript music bot with full Lavalink v4 support, featuring 60+ commands, autoplay, sleep timer, history tracking, and advanced queue management",
  "version": "1.5.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node .",
    "dev": "nodemon run node --trace-warnings .",
    "test": "node . mode=test",
    "commit": "cz",
    "docker:build": "docker build --tag rasavedic-music-bot .",
    "docker:shell": "docker run -it --rm rasavedic-music-bot sh",
    "docker:start": "docker run -it -p 3000:3000 --env-file ./.env -d --name rasavedic-music-bot rasavedic-music-bot",
    "docker:restart": "docker restart rasavedic-music-bot",
    "docker:stop": "docker stop rasavedic-music-bot",
    "docker:kill": "docker rm -f rasavedic-music-bot",
    "docker:purge": "docker rm -fv rasavedic-music-bot",
    "docker:logs": "docker logs rasavedic-music-bot -f",
    "docker:image": "docker image tag rasavedic-music-bot RasaVedic/rasavedic-music-bot",
    "docker:push": "docker push RasaVedic/rasavedic-music-bot",
    "docker:update": "git pull && npm install && npm run docker:stop && npm run docker:kill && npm run docker:build && npm run docker:start",
    "docker:dev:build": "docker build --tag rasavedic-music-bot-dev -f ./.devcontainer/.Dockerfile .",
    "docker:dev:start": "docker run -it --rm -v $(pwd):/app -v /app/node_modules -p 3000:3000 -p 9229:9229 -w /app rasavedic-music-bot-dev",
    "pm2:start": "pm2 start --name=rasavedic-music-bot npm -- run start",
    "pm2:stop": "pm2 stop rasavedic-music-bot",
    "pm2:purge": "pm2 stop rasavedic-music-bot && pm2 delete rasavedic-music-bot && pm2 reset rasavedic-music-bot",
    "pm2:logs": "pm2 logs --lines 300 rasavedic-music-bot",
    "pm2:logsError": "pm2 logs --err --lines 300 rasavedic-music-bot",
    "lint": "eslint src",
    "linter": "eslint src --fix",
    "writeLinter": "eslint src --output-file linter-output.txt"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.5.1",
    "@discordjs/rest": "^2.4.0",
    "@mirasaki/logger": "file:./vendor/@mirasaki/logger",
    "chalk": "^4.1.2",
    "common-tags": "^1.8.2",
    "discord-player": "^6.7.1",
    "discord.js": "^14.17.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "lokijs": "^1.5.12",
    "mediaplex": "^1.0.0",
    "shoukaku": "^4.1.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.18.0",
    "eslint-plugin-sonarjs": "^3.0.1",
    "nodemon": "^3.1.9",
    "semantic-release": "^24.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RasaVedic/rasavedic-music-bot.git"
  },
  "keywords": [
    "nodejs",
    "bot-template",
    "template",
    "boilerplate",
    "discord-api",
    "typings",
    "discord",
    "discordjs",
    "v14",
    "discord-bot",
    "music-bot",
    "music",
    "lavalink",
    "lavalink-v4",
    "shoukaku",
    "autoplay",
    "sleep-timer",
    "queue-management",
    "slash-commands",
    "buttons",
    "modals",
    "autocomplete",
    "context-menus",
    "select-menus",
    "documented"
  ],
  "author": "RasaVedic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RasaVedic/rasavedic-music-bot/issues"
  },
  "homepage": "https://github.com/RasaVedic/rasavedic-music-bot#readme",
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engineStrict": true,
  "engines": {
    "node": ">=20.0.0 <24.0.0"
  }
}
